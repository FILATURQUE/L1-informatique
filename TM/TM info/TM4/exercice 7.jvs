void main() {
    int[] tab;
    int k = readInteger("Taille de la table");
    tab = new int[k];
    for (int i = 0; i < tab.length; i++) {
        tab[i] = readInteger("valeur de tab 1");
    }
    int o = 0;
    for (int j = 0; j < tab.length; j++) {
        if (tab[j] == tab[tab.length]) {
            print("Le tableau est un palindrome");
        } else {
            print("Non");
        }
        o++;
    }
}
